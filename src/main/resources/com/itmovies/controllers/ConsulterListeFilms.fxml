<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.itmovies.controllers.ConsulterListeFilmsController">
   <children>
      <TableView fx:id="table" layoutX="46.0" layoutY="180.0" onMouseEntered="#onMouseEnteredOnTable" prefHeight="394.0" prefWidth="907.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
      <Button fx:id="retourBtn" layoutX="46.0" layoutY="14.0" mnemonicParsing="false" onAction="#onRetourBtnClick" prefHeight="36.0" prefWidth="62.0" text="Retour" />
      <Button fx:id="acheterBtn" disable="true" layoutX="818.0" layoutY="110.0" mnemonicParsing="false" onAction="#onAcheterBtnClick" prefHeight="49.0" prefWidth="92.0" text="Acheter" />
   </children>
</AnchorPane>
