<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.GaussianBlur?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onMouseEntered="#onMouseHoverPane" prefHeight="533.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.itmovies.controllers.HistoriqueCoteClientController">
   <children>
      <ImageView fitHeight="618.0" fitWidth="832.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@./assets/ccc.jpg" />
         </image>
         <effect>
            <GaussianBlur />
         </effect>
      </ImageView>
      <Button fx:id="retourBtn" layoutX="31.0" layoutY="14.0" mnemonicParsing="false" onAction="#onRetourBtnClick" prefHeight="44.0" prefWidth="69.0" text="Back" />
      <TableView fx:id="table" layoutX="31.0" layoutY="105.0" prefHeight="473.0" prefWidth="770.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
