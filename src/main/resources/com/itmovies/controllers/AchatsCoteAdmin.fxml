<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.itmovies.controllers.AchatsCoteAdminController">
   <children>
      <TableView fx:id="table" layoutX="36.0" layoutY="229.0" prefHeight="345.0" prefWidth="830.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="retourBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onRetourBtnClick" prefHeight="37.0" prefWidth="66.0" text="Retour" />
      <Pane layoutX="666.0" layoutY="52.0" prefHeight="152.0" prefWidth="200.0">
         <children>
            <Button fx:id="validerBtn" disable="true" layoutX="60.0" layoutY="28.0" mnemonicParsing="false" onAction="#onValiderBtnClick" prefHeight="38.0" prefWidth="81.0" text="Valider" />
            <Button fx:id="refuserBtn" disable="true" layoutX="60.0" layoutY="76.0" mnemonicParsing="false" onAction="#onRefuserBtnClick" prefHeight="38.0" prefWidth="81.0" text="Refuser" />
         </children>
      </Pane>
   </children>
</AnchorPane>
