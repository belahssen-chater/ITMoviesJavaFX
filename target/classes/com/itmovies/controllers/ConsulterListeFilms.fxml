<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="892.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.itmovies.controllers.ConsulterListeFilmsController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="990.0" layoutX="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@./assets/images.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="table" layoutX="46.0" layoutY="180.0" onMouseEntered="#onMouseEnteredOnTable" prefHeight="394.0" prefWidth="791.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
      <Button fx:id="retourBtn" layoutX="46.0" layoutY="14.0" mnemonicParsing="false" onAction="#onRetourBtnClick" prefHeight="36.0" prefWidth="62.0" text="Back" textFill="#6e69ae">
         <font>
            <Font name="Bernard MT Condensed" size="12.0" />
         </font></Button>
      <Button fx:id="acheterBtn" disable="true" layoutX="716.0" layoutY="110.0" mnemonicParsing="false" onAction="#onAcheterBtnClick" prefHeight="49.0" prefWidth="92.0" text="Buy" textFill="#6e69ae">
         <font>
            <Font name="Bookman Old Style Bold Italic" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
